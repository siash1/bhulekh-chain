    "contract": {
        "name": "TitleProofAnchor",
        "desc": "\n    Stores Fabric state roots on Algorand for independent verification.\n\n    Global state layout:\n        anchor_authority   - Address of the account authorized to submit anchors\n        total_anchors      - Running count of all anchors submitted\n        last_anchor_round  - Algorand round number of the most recent anchor\n\n    Each anchor_state call records a batch of Fabric blocks (identified by\n    state_code, channel_id, block range, state root hash, and transaction\n    count). The state root itself is passed as an argument and is available\n    in the transaction log for off-chain verifiers to inspect. On-chain, the\n    contract tracks the anchor sequence number and the Algorand round for\n    temporal ordering.\n    ",
        "methods": [
            {
                "name": "initialize",
                "args": [
                    {
                        "type": "address",
                        "name": "authority",
                        "desc": "The Algorand address that will be permitted to submit state root anchors and manage title ASAs."
                    }
                ],
                "readonly": false,
                "returns": {
                    "type": "void"
                },
                "desc": "Set the authorized anchor account. Can only be called once by the\napplication creator."
            },
            {
                "name": "anchor_state",
                "args": [
                    {
                        "type": "string",
                        "name": "state_code",
                        "desc": "Indian state code (e.g., \"UP\", \"MH\", \"DL\")"
                    },
                    {
                        "type": "string",
                        "name": "channel_id",
                        "desc": "Fabric channel name (e.g., \"land-registry-channel\")"
                    },
                    {
                        "type": "uint64",
                        "name": "fabric_block_start",
                        "desc": "First Fabric block number in this anchor batch"
                    },
                    {
                        "type": "uint64",
                        "name": "fabric_block_end",
                        "desc": "Last Fabric block number in this anchor batch"
                    },
                    {
                        "type": "byte[]",
                        "name": "state_root",
                        "desc": "Merkle root hash of all state changes in the batch"
                    },
                    {
                        "type": "uint64",
                        "name": "tx_count",
                        "desc": "Number of Fabric transactions in this batch"
                    }
                ],
                "readonly": false,
                "returns": {
                    "type": "uint64",
                    "desc": "The anchor sequence number (1-indexed)."
                },
                "desc": "Anchor a Fabric state root to Algorand.\nThis is the core anchoring operation. The middleware batches Fabric blocks (typically every N blocks or every M minutes), computes a Merkle root of all state changes in that batch, and submits it here.\nThe state_root bytes are recorded in the transaction arguments and are therefore permanently available via the Algorand Indexer. The on-chain global state only tracks the anchor count and last round for efficiency."
            },
            {
                "name": "get_anchor_count",
                "args": [],
                "readonly": true,
                "returns": {
                    "type": "uint64",
                    "desc": "The total number of anchors as an ARC4-encoded UInt64."
                },
                "desc": "Get total number of state root anchors submitted to this contract.\nThis is a read-only method that does not modify state. It can be called by anyone without a transaction fee (via simulate)."
            },
            {
                "name": "rotate_authority",
                "args": [
                    {
                        "type": "address",
                        "name": "new_authority",
                        "desc": "The new Algorand address to authorize for anchoring."
                    }
                ],
                "readonly": false,
                "returns": {
                    "type": "void"
                },
                "desc": "Rotate the anchor authority to a new address.\nThis allows key rotation without redeploying the contract. Only the current authority can perform this operation. After rotation, the old authority address can no longer submit anchors."
            }
        ],
        "networks": {}
    },
    "bare_call_config": {
        "no_op": "CREATE"
    }
}